{
  "name": "petmatch-server",
  "version": "1.0.0",
  "description": "Server-side components for Petfinder-Database-Distributor integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "start": "node dist/app.js",
    "worker": "node dist/index.js",
    "server": "npm run build && node dist/app.js",
    "server:watch": "tsc --watch & nodemon dist/app.js",
    "backup": "npm run build && node dist/index.js backup",
    "backup:list": "npm run build && node dist/index.js list",
    "backup:scheduler": "npm run build && node dist/index.js scheduler",
    "sync": "npm run build && node dist/index.js sync",
    "ingest": "npm run build && node dist/index.js ingest"
  },
  "keywords": [
    "petfinder",
    "pet-match",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.2"
  }
}


